<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ReactNative.Hermes.Windows</id>
    <version>$Version$</version>
    <description>Windows implementation of Hermes JavaScript Engine for use with ReactNative (VERSION_DETAILS)</description>
    <authors>Facebook, Microsoft</authors>
    <projectUrl>https://github.com/microsoft/hermes-windows</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <copyright>&#169; Microsoft Corporation. All rights reserved.</copyright>
    <repository type="git" url="$RepoUri$" commit="$CommitId$" />
  </metadata>
  <files>
    <file src="$nugetroot$\lib\native\uwp\release\arm64\**\*.*"
          target="lib/native/uwp/arm64"
          exclude="**\*.manifest;**\*.iobj;**\*.ipdb;**\*.pdb;**\*.exp;**\*.ilk" />
    <file src="$nugetroot$\lib\native\uwp\release\x64\**\*.*"
          target="lib/native/uwp/x64"
          exclude="**\*.manifest;**\*.iobj;**\*.ipdb;**\*.pdb;**\*.exp;**\*.ilk" />
    <file src="$nugetroot$\lib\native\uwp\release\x86\**\*.*"
          target="lib/native/uwp/x86"
          exclude="**\*.manifest;**\*.iobj;**\*.ipdb;**\*.pdb;**\*.exp;**\*.ilk" />
    <file src="$nugetroot$\lib\native\win32\release\arm64\**\*.*"
          target="lib/native/win32/arm64"
          exclude="**\*.manifest;**\*.iobj;**\*.ipdb;**\*.pdb;**\*.exp;**\*.ilk" />
    <file src="$nugetroot$\lib\native\win32\release\x64\**\*.*"
          target="lib/native/win32/x64"
          exclude="**\*.manifest;**\*.iobj;**\*.ipdb;**\*.pdb;**\*.exp;**\*.ilk" />
    <file src="$nugetroot$\lib\native\win32\release\x86\**\*.*"
          target="lib/native/win32/x86"
          exclude="**\*.manifest;**\*.iobj;**\*.ipdb;**\*.pdb;**\*.exp;**\*.ilk" />

    <file src="$nugetroot$\build\native\include\**\*.*"
          target="build/native/include" />
    <file src="$nugetroot$\build\native\ReactNative.Hermes.Windows.targets"
          target="build/native/ReactNative.Hermes.Windows.targets" />

    <file src="$nugetroot$\license\*" target="license"/>
    <file src="$nugetroot$\tools\**\*.*" target="tools"/>
  </files>
</package>
